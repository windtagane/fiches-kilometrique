{
  "name": "fiches-kilometrique",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all clean transpile",
    "start-electron": "electron ./dist/server/bin/www.js",
    "start-electron-dev": "electromon ./dist/server/bin/www.js",
    "dev": "npm-run-all build start-electron",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile-server": "babel ./server --out-dir dist/server",
    "transpile-src": "babel ./src --out-dir dist/src",
    "transpile": "npm-run-all transpile-server transpile-src",
    "clean": "rimraf dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.5",
    "electromon": "^1.0.10",
    "electron": "^10.1.3",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0"
  }
}
